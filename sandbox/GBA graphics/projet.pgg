#Fichier de projet GBA Graphics

#Useful informations about the last session
 .section infos
  Directory = "D:\devkitPro\diablo_project\GBA graphics"
  Script = "script.spt"
  UserHelp = "lisezmoi.htm"

  convert.maskColor = RGB(255,0,255)
  convert.lighting   GAMMA=RGB(1.000,1.000,1.000)  ADD=RGB(0.0,0.0,0.0)  ALPHA=RGB(0.0,0.0,0.0)
  convert.mode = 0

  system.saveMapsParamsAuto = false
  system.MaxMemory = 32 MB
  Format = "Standard"

  DefineModel "Default"		{
  }
  Model = "Default"

  Platform = "Game Boy Advance, Nintendo"
  GBA.paletteMask = false
  GBA.mirrorMask = false
  GBA.align = 0
  End

